					<div class="search">
						
					</div>
					<div class="related-posts">
						<br>
						<h3>Related Posts</h3>

						<% list.forEach(function (b) { %>
						<div class="related-post">
							<div class="related-post-left">
								<a href="/berita/<%= b.id_Posting %>"><img src="/uploads/artikel/<%= b.Gambar %>" alt="" style="width: 240px; height: 135px"></a>
							</div>
							<div class="related-post-right">
								<h4><a href="/berita/<%= b.id_Posting %>"><%= b.Judul %></a></h4>
								<p ><%= b.artikel.substring(0,60) %></p>
							</div>
							<div class="clearfix"> </div>
						</div>
						<% }); %>

					</div>
				</div>
			</div>
			
		</div>
