<section class="team" >
    <div class="container">
        <div class="wthree_head_section">
            <h3 class="w3l_header">Skuad Persipura Jayapura 2018/2019</h3>  
        </div>
        <div class="team-grid-top">
            <h2>Staff Klub</h2>
            <p>&nbsp</p>
          
            <div class="col-md-3 team1">
                <div class="inner-team1">
                    <img src="images/bttm.jpg" alt="" />
                    <h3>Benhur T mano</h3>
                    <h4>Chairman</h4>
                    <p>indonesia</p>
                </div>
            </div>
            <div class="col-md-3 team1">
                <div class="inner-team1">
                    <img src="images/rudi.jpg" alt="" />
